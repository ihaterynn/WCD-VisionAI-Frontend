{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://194.31.53.238:8080/$1"
    },
    {
      "src": "/(.*)",
      "dest": "./index.html"
    }
  ]
}