{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
  { "source": "/api/recommendations/:path*", "destination": "http://194.31.53.238:8080/recommendations/:path*" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.html"
    }
  ]
}